<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<StackPane stylesheets="@application.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"  fx:controller="javafxtest.LoginControl">
   <children>
      <Pane prefHeight="300.0" prefWidth="300.0" style="-fx-background-color:#002266">
         <children>
            <TextField fx:id="id" layoutX="80.0" layoutY="112.0" />
            <PasswordField fx:id="pw" layoutX="80.0" layoutY="153.0" prefHeight="23.0" prefWidth="137.0" />
            <Button fx:id="joinbutton" layoutX="90.0" layoutY="192.0" mnemonicParsing="false" text="Join" />
            <Button fx:id="loginbutton" layoutX="148.0" layoutY="192.0" mnemonicParsing="false" text="Login" />
            <ImageView fitHeight="109.0" fitWidth="181.0" layoutX="70.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../res/logo2.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
   </children>
</StackPane>
